  }

  .carousel {
    // width: 350px;
    padding: 5px 10px 25px 5px;
  }
  .car-indic {
    width: 66px !important;
    height: 42px !important;
    background-repeat: no-repeat;
    background-position: center;
    background-size: 100% auto;
  }

  .img-ic-3 {
    width: 25px;
    height: 25px;
  }
  .carousel-button {
    margin: 20px 0px;
  }
}

