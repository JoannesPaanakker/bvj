@media screen and (min-width: 601px) {
  .carousel {
    width: 450px;
  }

  .carousel-indicators .active {
    border-radius: unset;
    margin-bottom: -6px;
    border-bottom: solid 6px lightgrey;
    background-color: lightgrey;
  }

  .indicators-container {
    width: 85vw;
    overflow: auto;
    margin: 30px 0px 0px -8%;
  }

  .carousel-div {
    // width: 900px;
    padding: 140px 10px 0px 10px;
    max-width: 720px;
    display: flex;
    justify-content: center;
  }

  .img-div {
    max-width: 500px;
  }

  .dummy-content {
    width: 120px;
    height: 80px;
    text-align: center;
    padding-top: 30px;
  }

  .ci-content{
    width: 245px;
    height: 78px;
    font-size: 8px;
    display: flex;
    justify-content: space-between;
  }

  .ci-content-cover {
    width: 120px;
    height: 80px;
  }

  .ci-content-l {
    // width: 120px;
    // height: 80px;
    // background: white;
  }

  .ci-content-r {
    width: 120px;
    height: 80px;
    background: white;
  }

  .car-indic {
    // margin: 0px 8px 0px 8px !important;
    width: 132px !important;
    height: 84px !important;
    border-radius: 0px !important;
    display: flex;
  }

  .img-ic-3 {
    width: 35px;
    height: 35px;
  }

  .glyphicon.glyphicon-chevron-left {
    display: none;
    margin-left: -30px;
    top: 40%;
    font-size: 40px;
  }

  .glyphicon.glyphicon-chevron-right {
    top: 40%;
    font-size: 40px;
  }

  .carousel-control.glyphicon-chevron-left {
  }

  .carousel-control.glyphicon-chevron-right {

  }

  .carousel-button-container {
    padding: 15px;
  }
}

@media screen and (max-width: 600px) {

  .carousel-indicators .active {
    border-radius: unset;
    // margin-bottom: -2px;
    // border-bottom: solid 2px lightgrey;
    background-color: lightgrey;
  }

  .carousel-div {
    // width: 900px;
    padding: 0px;
  }

  .item1 {
    padding: 0px;
  }

  .indicators-container {
    // width: 325px;
    overflow: auto;
    margin: 5px 0px 0px 5px;
  }

  .dummy-content {
    width: 60px;
    height: 40px;
  }

  .ci-content{
    width: 122px;
    height: 39px;
    font-size: 3px;
